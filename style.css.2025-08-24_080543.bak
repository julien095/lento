.navbar{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 1rem 2rem;
    background-color:#003366;
    color: aliceblue;

}

.menu a{
    color: aliceblue;
	text-decoration:none;

}

.menu ul{
    list-style:none;
	display:flex;
	gap:1.5rem;

}
.logo{
    display: flex;
    align-items:center;
    gap:10px	
}
.logo-img{
     height:50px;
	 width: 50px;
	 border-radius:50%;
	 object-fit:cover;
	 border:5px;
	 border-style:solid;
	 border-color:#6a1b9a;
	 
}
.logo-text{
     font-size:10px
}
.hero{
      position: relative;
  background-image: data:image/jpeg;base64,; /* Chanje pou imaj pa w si ou vle */
  background-size: cover;
  background-position: center;
  height: 100vh;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  padding: 2rem;


}







a:hover{
     color:rgb(249,9,9);
}