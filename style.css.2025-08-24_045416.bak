.navbar{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 1rem 2rem;
    background-color:#003366;
    color: aliceblue;

}

.menu a{
    color: aliceblue;
	text-decoration:none;

}

.menu ul{
    list-style:none;
	display:flex;
	gap:1.5rem;

}
.logo{
    display: flex;
    align-items:center;
    gap:10px	
}
.logo-img{
     height:40px
}
.logo-text{
     font-size:10px
}








a:hover{
     color:rgb(249,9,9);
}